# Google Cloud Configuration
# Path to your service account JSON file (relative to this directory)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Your Google Cloud Project ID (not the project name)
GOOGLE_CLOUD_PROJECT=your-project-id

# Google Cloud location (typically 'global' for most use cases)
GOOGLE_CLOUD_LOCATION=global

# Google API Key for Gemini models
GOOGLE_API_KEY=

# Enable Vertex AI usage (required for gemini-embedding-001)
GOOGLE_GENAI_USE_VERTEXAI=True

# Supabase Configuration
# Your Supabase project URL
SUPABASE_URL=https://your-project.supabase.co

# Your Supabase service key (not the anon key)
SUPABASE_SERVICE_KEY=your-service-key

# Optional: Application Configuration
# Override default settings if needed
# LOG_LEVEL=INFO
# MAX_CHUNK_SIZE=1000
# CHUNK_OVERLAP=100 